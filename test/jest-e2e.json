{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFilesAfterEnv": [
        "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
        "^../../(.*)$": "<rootDir>/src/$1",
        "^@prisma/(.*)$": "<rootDir>/src/prisma/$1"
    }
}